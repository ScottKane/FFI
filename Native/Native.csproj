<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    </PropertyGroup>
    
    <PropertyGroup>
        <CargoPackage>ffi</CargoPackage>
<!--        <CargoChannel>nightly</CargoChannel>-->
        <CargoWorkspacePath>$(MSBuildThisFileDirectory)/Rust</CargoWorkspacePath>
    </PropertyGroup>

    <Import Project="Rust.targets" />

</Project>
